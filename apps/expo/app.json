{
  "expo": {
    "scheme": "fetch",
    "web": {
      "bundler": "metro"
    },
    "name": "fetch",
    "slug": "expo-app",
    "icon": "./assets/FetchLogoTransparentWithText.png",
    "userInterfaceStyle": "automatic",
    "sourceExts": ["js", "json", "ts", "tsx", "jsx", "vue"],
    "productionApiUrl": "https://fetch-kodokoto.vercel.app/",
    "plugins": [
      [
        "expo-location",
        {
          "locationAlwaysAndWhenInUsePermission": "Allow Fetch to use your location."
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "1c17d79a-5d82-4f53-a8bd-9e6dca56f54d"
      }
    },
    "android": {
      "package": "com.kodokoto.fetch"
    },
    "ios": {
      "bundleIdentifier": "com.kodokoto.fetch"
    }
  }
}
