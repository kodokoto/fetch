{
  "expo": {
    "scheme": "fetch",
    "web": {
      "bundler": "metro"
    },
    "name": "expo-app",
    "slug": "expo-app",
    "userInterfaceStyle": "automatic",
    "sourceExts": ["js", "json", "ts", "tsx", "jsx", "vue"],
    "plugins": [
      [
        "expo-location",
        {
          "locationAlwaysAndWhenInUsePermission": "Allow Fetch to use your location."
        }
      ]
    ]
  }
}
